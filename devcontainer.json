{
  "name": "Java Development Environment",
  "image": "mcr.microsoft.com/vscode/devcontainers/java:11",
  "settings": {
    "java.home": "/usr/lib/jvm/java-11-openjdk-amd64"
  },
  "extensions": [
    "vscjava.vscode-java-pack",
    "redhat.java",
    "eamodio.gitlens"
  ],
  "forwardPorts": [8080],
  "remoteUser": "vscode",
  "postCreateCommand": "mvn clean install"
}
